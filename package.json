{
  "name": "gradecalc",
  "version": "1.0.0",
  "description": "GradeCalc es aplicación web para calcular la nota que necesitas en cada examen para aprobar una asignatura. Originalmente hecha para la FIB, Facultad de Informàtica de Barcelona",
  "main": "index.html",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "predist": "rm -rf dist/* && touch dist/.gitkeep",
    "dist": "run-p dist:*",
    "dist:assets": "cp -r src/assets dist/",
    "dist:scripts": "run-p dist:scripts:*",
    "dist:scripts:gulp": "gulp js",
    "dist:scripts:sw": "cp src/scripts/sw.js dist/",
    "dist:scripts:lib": "gulp libs",
    "dist:styles": "gulp css",
    "dist:lib": "cp -r lib dist/lib",
    "dist:layout": "cp src/index.html dist/",
    "dist:webmanifest": "cp src/site.webmanifest dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mauriciabad/GradeCalc.git"
  },
  "author": "Maurici Abad Gutierrez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mauriciabad/GradeCalc/issues"
  },
  "homepage": "https://github.com/mauriciabad/GradeCalc#readme",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.5",
    "browserify": "^16.5.0",
    "browserslist": "^4.7.0",
    "del": "^5.1.0",
    "eslint": "^6.4.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-imagemin": "^6.1.0",
    "gulp-minify": "^3.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-useref": "^3.1.6",
    "npm-run-all": "^4.1.5",
    "run-sequence": "^2.2.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "core-js": "^3.2.1",
    "regenerator-runtime": "^0.13.3"
  }
}
